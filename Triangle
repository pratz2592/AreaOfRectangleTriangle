class Triangle extends Rectangle 
                      { 
                          float a; 
                          Triangle(int v,int u) 
                             { 
                                 super(u,v); 
                             } 
                                 float getTriangle() 
                                    { 
                                        a=(float)l/2*l*b; 
                                        return (a); 
                                    } 
                     } 
